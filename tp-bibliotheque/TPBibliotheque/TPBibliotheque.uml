      <packagedElement xmi:type="uml:Class" xmi:id="_tnT4MGAHEe2WXM2LDn-oyg" name="MenuFrame">
        <ownedAttribute xmi:id="_tsTs8GAHEe2WXM2LDn-oyg" name="grid" visibility="package" type="_trcKQWAHEe2WXM2LDn-oyg" association="_ttjDEWAHEe2WXM2LDn-oyg"/>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tsyOEmAHEe2WXM2LDn-oyg" name="MenuFrame" specification="_tnUfQGAHEe2WXM2LDn-oyg">
          <ownedParameter xmi:id="_tsyOE2AHEe2WXM2LDn-oyg" name="isManager">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
