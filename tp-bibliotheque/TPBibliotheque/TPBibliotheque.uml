            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
          </ownedParameter>
          <ownedParameter xmi:id="_tswY5mAHEe2WXM2LDn-oyg" name="row">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
          </ownedParameter>
          <language>java</language>
          <body>Text text = new Text(comment);&#xD;
     text.setFont(Font.font(&quot;Tahoma&quot;, FontWeight.NORMAL, 12));&#xD;
     gridUp.add(text, column, row);&#xD;
	</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tswY52AHEe2WXM2LDn-oyg" name="addButtonToTable" specification="_tnHD4GAHEe2WXM2LDn-oyg">
          <language>java</language>
          <body>TableColumn&lt;Book, Void> colBtn = new TableColumn(&quot;&quot;);&#xD;
		&#xD;
     Callback&lt;TableColumn&lt;Book, Void>, TableCell&lt;Book, Void>> cellFactory = new Callback&lt;TableColumn&lt;Book, Void>, TableCell&lt;Book, Void>>() {&#xD;
         @Override&#xD;
         public TableCell&lt;Book, Void> call(final TableColumn&lt;Book, Void> param) {&#xD;
             final TableCell&lt;Book, Void> cell = new TableCell&lt;Book, Void>() {&#xD;
                 private final Button btn = new Button(&quot;borrow&quot;);&#xD;
                 {&#xD;
                     btn.setOnAction((ActionEvent event) -> {&#xD;
                         Book data = getTableView().getItems().get(getIndex());&#xD;
                         Controller.getController().borrowBook(data);&#xD;
                         &#xD;
                 		Alert alert = new Alert(AlertType.INFORMATION);&#xD;
                 		alert.setTitle(null);&#xD;
                 		alert.setHeaderText(null);&#xD;
                 		alert.setContentText(&quot;Book successfully borrowed!&quot;);&#xD;
                 		alert.showAndWait();&#xD;
                 		&#xD;
                     	new MenuFrame(isManager);&#xD;
                     	borrowStage.close();&#xD;
                     });&#xD;
                 }&#xD;
&#xD;
                 @Override&#xD;
                 public void updateItem(Void item, boolean empty) {&#xD;
                     super.updateItem(item, empty);&#xD;
                     if (empty) &#xD;
                         setGraphic(null);&#xD;
                     else &#xD;
                         setGraphic(btn);&#xD;
                 }&#xD;
             };&#xD;
             return cell;&#xD;
         }&#xD;
		};&#xD;
		&#xD;
     colBtn.setCellFactory(cellFactory);&#xD;
     table.getColumns().add(colBtn);&#xD;
	</body>
        </ownedBehavior>
