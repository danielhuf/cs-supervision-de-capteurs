        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tsodEGAHEe2WXM2LDn-oyg" name="checkCredentials" specification="_tmagUWAHEe2WXM2LDn-oyg">
          <ownedParameter xmi:id="_tsodEWAHEe2WXM2LDn-oyg" name="u">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
          </ownedParameter>
          <ownedParameter xmi:id="_tsodEmAHEe2WXM2LDn-oyg" name="password">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
          </ownedParameter>
          <ownedParameter xmi:id="_tsodE2AHEe2WXM2LDn-oyg" direction="return">
            <name xsi:nil="true"/>
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
          </ownedParameter>
          <language>java</language>
          <body>try {&#xD;
			String sql = &quot;SELECT * FROM Users WHERE Email = ? AND ID = ?&quot;;&#xD;
			PreparedStatement stmt = con.prepareStatement(sql);&#xD;
			stmt.setString(1, u);&#xD;
			stmt.setString(2, password);&#xD;
			ResultSet rs = stmt.executeQuery();&#xD;
			&#xD;
			if (rs.next()) {  // an user with such credentials was found&#xD;
				if (rs.getString(4).equals(&quot;Manager&quot;)) {&#xD;
					Controller.getController().setManager(true);&#xD;
				}&#xD;
				else {&#xD;
					Controller.getController().setManager(false);&#xD;
				}&#xD;
				user = new User(rs.getString(1), rs.getString(2), rs.getString(3), rs.getString(4), rs.getInt(5), rs.getBoolean(6));&#xD;
				return true;&#xD;
			}&#xD;
			&#xD;
			else { return false; }&#xD;
			&#xD;
		} &#xD;
 	catch (SQLException e) {&#xD;
			System.out.println(e);&#xD;
			return false;&#xD;
		}&#xD;
 </body>
        </ownedBehavior>
